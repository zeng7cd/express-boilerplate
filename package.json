{
  "name": "express-boilerplate",
  "version": "0.0.1",
  "description": "An Express boilerplate backend",
  "author": "zeng7cd",
  "repository": "express-boilerplate-2025",
  "main": "index.ts",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm run generate:controllers && tsc && tsup",
    "start:dev": "pnpm run generate:controllers && cross-env NODE_ENV=development node --import=tsx --watch src/index.ts",
    "start:prod": "node dist/index.js",
    "format": "prettier --write .",
    "check": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit --skipLibCheck",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "db:create": "tsx scripts/create-database.ts",
    "db:generate": "drizzle-kit generate",
    "db:push": "tsx src/core/database/migrate.ts",
    "db:migrate": "tsx src/core/database/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/core/database/seed.ts",
    "generate:controllers": "tsx scripts/generate-controllers.ts",
    "generate:secrets": "tsx scripts/generate-secrets.ts"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^3.0.4",
    "bcryptjs": "^3.0.3",
    "chalk": "^5.6.2",
    "compression": "^1.8.1",
    "cors": "2.8.5",
    "cron": "^4.4.0",
    "cron-parser": "^5.4.0",
    "dayjs": "^1.11.19",
    "dotenv": "17.2.3",
    "drizzle-orm": "^0.45.1",
    "express": "5.2.1",
    "express-rate-limit": "8.2.1",
    "helmet": "8.1.0",
    "http-status-codes": "2.3.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "lodash-es": "^4.17.22",
    "mysql2": "^3.16.0",
    "nanoid": "^5.1.6",
    "pino": "10.1.0",
    "pino-http": "11.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "ua-parser-js": "^2.0.7",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.0.3",
    "@types/supertest": "6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitest/coverage-v8": "4.0.16",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "fast-check": "^4.4.0",
    "globals": "^16.5.0",
    "openapi-types": "^12.1.3",
    "pino-pretty": "13.1.3",
    "prettier": "^3.7.4",
    "supertest": "7.1.4",
    "swagger-ui-express": "^5.0.1",
    "tsup": "8.5.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "6.0.2",
    "vitest": "4.0.16"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "outDir": "dist",
    "format": [
      "esm",
      "cjs"
    ],
    "target": "es2020",
    "sourcemap": true,
    "clean": true,
    "dts": true,
    "splitting": false,
    "skipNodeModulesBundle": true
  },
  "packageManager": "pnpm@10.12.1"
}
